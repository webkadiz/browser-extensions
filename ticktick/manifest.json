{
	"manifest_version": 2,
	"name": "ticktick",
	"version": "1.0",
	"description": "Shortcuts for TickTick",
	"content_scripts": [
		{
			"matches": ["https://ticktick.com/*"],
			"js": [
				"shortcuts.js",
				"timer.js",
				"list-navigation.js",
				"time-planner-popup.js",
				"search-popup.js"
			],
			"css": ["injected-style.css"]
		}
	]
}
